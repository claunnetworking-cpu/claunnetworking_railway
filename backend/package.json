{
  "name": "claunnetworking-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend API para ClaunNetworking Oportunidades",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "db:push": "drizzle-kit generate && drizzle-kit migrate",
    "db:seed": "node scripts/seed.mjs",
    "lint": "eslint src --ext .ts",
    "test": "vitest run"
  },
  "dependencies": {
    "express": "^4.21.2",
    "express-cors": "^1.0.0",
    "express-rate-limit": "^8.2.1",
    "dotenv": "^17.2.2",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.15.0",
    "drizzle-orm": "^0.44.5",
    "zod": "^4.1.12",
    "uuid": "^9.0.1",
    "axios": "^1.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.7.0",
    "@types/jsonwebtoken": "^9.0.10",
    "typescript": "^5.9.3",
    "tsx": "^4.19.1",
    "drizzle-kit": "^0.31.4",
    "vitest": "^2.1.4"
  }
}
